#!/usr/bin/python3

from gpiozero import LED
from time import sleep

pair1 = LED(11)
pair2 = LED(9)
pair3 = LED(10)
pair4 = LED(7)
pair5 = LED(8)

while True:
    pair1.on()
    sleep(randint(2,10)/200)
    #pair1.off()
    pair2.on()
    sleep(randint(2,10)/200)
    #pair2.off()
    pair3.on()
    sleep(randint(2,10)/200)
    #pair3.off()
    pair4.on()
    sleep(randint(2,10)/200)
    #pair4.off()
    pair5.on()
    sleep(randint(2,10)/200)
    pair5.off()
    pair4.off()
    pair3.off()
    pair2.off()
    pair1.off()
    sleep(randint(2,10)/200)
